{
  "manifest_version": 2,
  "name": "Copy equation direct to word file",
  "version": "3.6",
  "description": "Copy MathML and LaTeX from math elements",
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "svg/*.svg",
    "css/*.css"
   
  ],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://chatgpt.com/*", "https://*.wikipedia.org/*"]
    }
  ]
}